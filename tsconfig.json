{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "module": "CommonJS",
    "lib": ["ES2020"],
    "target": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "rootDir": "src",
    "sourceRoot": "src",
    "sourceMap": true,
    "outDir": "build"
  },
  "include": ["src/**/*.ts"],
  "exclude": ["**/*.test.ts"]
}
